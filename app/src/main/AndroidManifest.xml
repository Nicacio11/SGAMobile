        </activity>
        <activity android:name=".ErroActivity"></activity>

        <meta-data
            android:name="com.google.android.gms.ads.ca-app-pub-5069015322638131~9977928263"
            android:value="ca-app-pub-5069015322638131/9620913034"/>
